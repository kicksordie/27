<h1>Listing votes</h1>

<table>
  <thead>
    <tr>
      <th>Knowledge sharing</th>
      <th>Passing difficulty</th>
      <th>Justice</th>
      <th>Jokes level</th>
      <th>Teacher</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.knowledge_sharing %></td>
        <td><%= vote.passing_difficulty %></td>
        <td><%= vote.justice %></td>
        <td><%= vote.jokes_level %></td>
        <td><%= vote.teacher %></td>
        <td><%= vote.user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
